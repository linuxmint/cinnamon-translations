#, c-format
msgid "You will be automatically logged out in %d second."
msgid_plural "You will be logged out in %d seconds."
msgstr[0] "Jūs būsite automatiškai atjungti po %d sekundės."
msgstr[1] "Jūs būsite automatiškai atjungti po %d sekundžių."
msgstr[2] "Jūs būsite automatiškai atjungti po %d sekundžių."

#: cinnamon-session-quit/cinnamon-session-quit.py:318
#, c-format
msgid "This system will be automatically shut down in %d second."
msgid_plural "This system will be shut down in %d seconds."
msgstr[0] "Sistema automatiškai išsijungs po %d sekundės."
msgstr[1] "Sistema automatiškai išsijungs po %d sekundžių."
msgstr[2] "Sistema automatiškai išsijungs po %d sekundžių."

#: cinnamon-session-quit/cinnamon-session-quit.py:324
#, c-format
msgid "This system will be automatically restarted in %d second."
msgid_plural "This system will be restarted in %d seconds."
msgstr[0] "Sistema bus automatiškai paleista iš naujo po %d sekundės."
msgstr[1] "Sistema bus automatiškai paleista iš naujo po %d sekundžių."
